.overlay-data {
  position: absolute;
  bottom: 0;
  padding: 15px;
  width: 100%;
}

.overlay-data a {
  color: #fff;
  font-size: 18px;
  text-transform: uppercase;
  font-weight: 600;

}

.overlay-data span {
  color: #fff;
  font-size: 14px;
  margin-top: 4px;
}

.hover-data {
  overflow: hidden;
  position: absolute;
  right: 0;
  top: 0;
  opacity: 0;
  height: 0;
  transition: .5s ease;
  width: 100%;
}

.trend-div:hover>.overlay-data {
  display: none;
}

.trend-div:hover>.hover-data {
  display: block;
  border-radius: 4px;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  padding: 20px 25px;
  background: rgba(50, 63, 85, 0.9);
  transition: .5s ease;
  height: 100%;
  opacity: 1
}

.hover-data h4 {
  color: #fff;
  font-size: 24px;
  font-weight: bold;
  text-transform: uppercase;
  margin-bottom: 20px;
}

.hover-data p {
