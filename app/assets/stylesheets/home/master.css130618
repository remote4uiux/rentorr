}

.custom-btn {
  padding: 10px 40px;
}

.round-btn {
  -webkit-border-radius: 100px;
  -moz-border-radius: 100px;
  border-radius: 100px;
}

.btn-dark-gray {
  -webkit-box-shadow: 0px 3px 20px 0px rgba(50, 50, 50, 0.33);
  -moz-box-shadow: 0px 3px 20px 0px rgba(50, 50, 50, 0.33);
  box-shadow: 0px 3px 20px 0px rgba(50, 50, 50, 0.33);
  color: #fff;
  font-size: 14px;
  font-weight: 400;
  text-shadow: 1px 1px 1px rgba(0, 0, 0, .3);
  /* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#3d4a60+0,3d4a60+100 */
  background: #3d4a60; /* Old browsers */
  background: -moz-linear-gradient(top, #3d4a60 0%, #3d4a60 100%); /* FF3.6-15 */
  background: -webkit-linear-gradient(top, #3d4a60 0%,#3d4a60 100%); /* Chrome10-25,Safari5.1-6 */
  background: linear-gradient(to bottom, #3d4a60 0%,#3d4a60 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#3d4a60', endColorstr='#3d4a60',GradientType=0 ); /* IE6-9 */
  display: inline-block;

}

.btn-dark-gray:hover, .btn-dark-gray:focus {

  background: #22a57d;
  -webkit-box-shadow: 0px 10px 54px -9px rgba(0, 0, 0, 0.4);
  -moz-box-shadow: 0px 10px 54px -9px rgba(0, 0, 0, 0.4);
  box-shadow: 0px 10px 54px -9px rgba(0, 0, 0, 0.4);
  color: #fff;
  background: #1ea07a;
  /* Old browsers */
  background: -moz-linear-gradient(-45deg, #1ea07a 0%, #45cd96 65%, #45cd96 100%);
  /* FF3.6-15 */
  background: -webkit-linear-gradient(-45deg, #1ea07a 0%, #45cd96 65%, #45cd96 100%);
  /* Chrome10-25,Safari5.1-6 */
  background: linear-gradient(135deg, #1ea07a 0%, #45cd96 65%, #45cd96 100%);
  /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1ea07a', endColorstr='#45cd96', GradientType=1);
  /* IE6-9 fallback on horizontal gradient */
}

.btn-gray-outer {

  color: #7b7d88;
  font-size: 14px;
  font-weight: 400;
  background:none;
  border: solid 1px #c7ccd1;
  display: inline-block;
}

.btn-gray-outer:hover, .btn-gray-outer:focus {
  background: #22a57d;
  -webkit-box-shadow: 0px 10px 54px -9px rgba(0, 0, 0, 0.4);
  -moz-box-shadow: 0px 10px 54px -9px rgba(0, 0, 0, 0.4);
  box-shadow: 0px 10px 54px -9px rgba(0, 0, 0, 0.4);
  color: #fff;
  background: #1ea07a; border: solid 1px transparent;
  /* Old browsers */
  background: -moz-linear-gradient(-45deg, #1ea07a 0%, #45cd96 65%, #45cd96 100%);
  /* FF3.6-15 */
  background: -webkit-linear-gradient(-45deg, #1ea07a 0%, #45cd96 65%, #45cd96 100%);
  /* Chrome10-25,Safari5.1-6 */
  background: linear-gradient(135deg, #1ea07a 0%, #45cd96 65%, #45cd96 100%);
  /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1ea07a', endColorstr='#45cd96', GradientType=1);
  /* IE6-9 fallback on horizontal gradient */
}

.btn-green-light {
  background: #1ea07a;
  /* Old browsers */
  background: -moz-linear-gradient(-45deg, #1ea07a 0%, #45cd96 65%, #45cd96 100%);
  /* FF3.6-15 */
  background: -webkit-linear-gradient(-45deg, #1ea07a 0%, #45cd96 65%, #45cd96 100%);
  /* Chrome10-25,Safari5.1-6 */
  background: linear-gradient(135deg, #1ea07a 0%, #45cd96 65%, #45cd96 100%);
  /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1ea07a', endColorstr='#45cd96', GradientType=1);
  /* IE6-9 fallback on horizontal gradient */
  color: #fff;
  font-size: 14px;
  font-weight: 400;
  text-shadow: 1px 1px 1px rgba(0, 0, 0, .3);
}

.btn-green-light:hover, .btn-green-light:focus {
  background: #22a57d;
  -webkit-box-shadow: 0px 10px 54px -9px rgba(0, 0, 0, 0.4);
  -moz-box-shadow: 0px 10px 54px -9px rgba(0, 0, 0, 0.4);
  box-shadow: 0px 10px 54px -9px rgba(0, 0, 0, 0.4);
  color: #fff;
  background: #1ea07a;
  /* Old browsers */
  background: -moz-linear-gradient(-45deg, #1ea07a 0%, #45cd96 65%, #45cd96 100%);
  /* FF3.6-15 */
  background: -webkit-linear-gradient(-45deg, #1ea07a 0%, #45cd96 65%, #45cd96 100%);
  /* Chrome10-25,Safari5.1-6 */
  background: linear-gradient(135deg, #1ea07a 0%, #45cd96 65%, #45cd96 100%);
  /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1ea07a', endColorstr='#45cd96', GradientType=1);
